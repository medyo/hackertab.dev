{
  "manifest_version": 3,
  "name": "Hackertab.dev - developer news",
  "description": "All developer news in one tab",
  "version": "1.26.2",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "host_permissions": [
    "https://*.hackertab.dev/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://hackertab.dev/*"
      ],
      "run_at": "document_start",
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "/logos/logo16.png",
    "32": "/logos/logo32.png",
    "48": "/logos/logo48.png",
    "128": "/logos/logo128.png"
  }
}
